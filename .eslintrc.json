{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prefer-const": 0,
    "space-before-function-paren": 0,
    "padded-blocks": 0,
    "eqeqeq": 0,
    "comma-dangle": 0,
    "no-undef": 0,
    "indent": 0,
    "no-unneeded-ternary": 1,
    "no-constant-condition": 0
  }
}
